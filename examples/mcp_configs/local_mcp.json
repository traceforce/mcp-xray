{
    "mcpServers": {
      "AWS Documentation": {
        "command": "uvx awslabs.aws-documentation-mcp-server@latest",
        "env": {
          "FASTMCP_LOG_LEVEL": "ERROR",
          "AWS_DOCUMENTATION_PARTITION": "aws"
        },
        "args": []
      },
      "playwright": {
        "command": "npx",
        "args": [
          "@playwright/mcp@latest",
          "--isolated",
          "--storage-state={path/to/storage.json}"
        ]
      }
    }
  }